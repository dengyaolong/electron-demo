{
  "name": "markdown-electron",
  "version": "1.0.1",
  "description": "A simple markdown editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack SimpleMarkdown-darwin-x64/SimpleMarkdown.app/Contents/Resources/app SimpleMarkdown-darwin-x64/SimpleMarkdown.app/Contents/Resources/app.asar",
    "build": "electron-packager . SimpleMarkdown --platform=darwin --arch=x64 --ignore=node_modules/electron-*"
  },
  "repository": "https://github.com/dengyaolong/electron-simple-markdown-editor.git",
  "keywords": [
    "Electron",
    "markdown"
  ],
  "author": "dyl",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.1.0"
  },
  "dependencies": {
    "marked": "^0.3.6"
  }
}
